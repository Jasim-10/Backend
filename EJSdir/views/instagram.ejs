<%- include("include/head.ejs") %>
<body>
    <h2>This page is belongs
        @<%= data.name %>
    </h2>
    <button>Follow</button>
    <button>Message</button>

    <p>Followers: <%= data.followers %> &nbsp;  Following: <%= data.following %></p>   
    <hr>

    <% for(let post of data.posts) {%>
        <img src="<%= post.image %>" alt="some img">
        <p>
            likes: <%= post.likes %> &nbsp; comments: <%= post.comments %>
        </p>
    <%}%>
</body>
</html>
